#---------------------------------------------------------------
#----------------------------Load Model----------------------
#---------------------------------------------------------------
GeomodellerTask {
    OpenProjectNoGUI {
        filename: "./Models_Prelim/Models_UWA.xml"
    }
}
#---------------------------------------------------------------
#----------------------------Compute Model----------------------
#---------------------------------------------------------------

GeomodellerTask {
    ComputeModel {
        SeriesList {
            node: "All" 
        }
        SectionList {
            node: "All"
        }
        FaultList {
            node: "All"
        }
        radius: 10.0
    }
}
#---------------------------------------------------------------
#--------------------------Save As Model------------------------
#---------------------------------------------------------------
GeomodellerTask {
    SaveLithologyVoxet {
        nx: 10
        ny: 10
        nz: 5
        LithologyVoxetFileStub: "./Litho_Voxet/LithoVoxet.vo"
    }
}
GeomodellerTask {
    RenderModel {
        nx:  10
        ny:  10
        nz:  5
        generateSurfaces: true
    }
}

GeomodellerTask {
    RenderModel {
        nx:  60
        ny:  60
        nz:  60
        generateSurfaces: true
        generateVolumes: false
        buildFormations: true
        buildFaults: true
        adaptToTopo: true
        topoLimited: true
        createShapes: true
        smoothingIterations: 1
    }
}

GeomodellerTask {
    Export3DShapes {
        file_type: TSURF
        output_directory: "./TSurf"
    }
}

GeomodellerTask {
    SaveProjectAs {
        filename: "./Models_Final/Models_UWA.xml"
    }
}
GeomodellerTask {
    CloseProjectNoGUI {
    }
}