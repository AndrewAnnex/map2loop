#---------------------------------------------------------------
#----------------------------Compute Model----------------------
#---------------------------------------------------------------
GeomodellerTask {
    ComputeModel {
        FaultList {
            node: "ALL"
        }
        SectionList {
            node: "Topography"
        }
        radius: 10.0
    }
}
GeomodellerTask {
    GetModelSummary {
        level: 2;
        filename: "summary.rpt";
    }
}
GeomodellerTask {
    RenderModel {
        nx:12;
        ny:12;
        nz:12;
        generateSurfaces: true;
        generateVolumes: false;
        topoLimited: true;
        adaptToTopo: true;
        buildFormations: false;
        buildFaults: true;
        buildDykes: false;
    }
}
