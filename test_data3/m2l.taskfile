name: "UWA_Intrepid"
description: "Automate_batch_Model"
    GeomodellerTask {
    CreateProject {
        name: "Hamersley"
        author: "Des/Mark"
        date: "23/10/2019  0: 0: 0"
        projection { map_projection: "GDA94 / MGA50"}
        version: "2.0"
        units: meters
        precision: 1.0
        Extents {
            xmin: 500000
            ymin: 7455500
            zmin: -3000
            xmax: 603000
            ymax: 7568000
            zmax: 1000
        }
        deflection2d: 0.001
        deflection3d: 0.001
        discretisation: 10.0
        referenceTop: false
        CustomDTM {
            Extents {
                xmin: 500000
                ymin: 7455500
                xmax: 603000
                ymax: 7568000
            }
            name: "Topography"
            filename {
                Grid_Name: "./dtm/hammersley_sheet_dtm.ers"
            }
            nx: 10
            ny: 10
        }
    }
}
